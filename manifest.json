{
  "name": "My TweetDeck",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://tweetdeck.twitter.com/*"
      ],
      "js": [
        "vendor/jquery.min.js",
        "config/config.js",
        "scripts/component.js",
        "scripts/clear_timeline.js",
        "scripts/filter_modal.js",
        "scripts/send_notice.js",
        "scripts/custom_timeline.js"
      ],
      "css": [
        "styles/clear_timeline.css",
        "styles/filter_modal.css",
        "styles/send_notice.css",
        "styles/custom_timeline.css"
      ]
    }
  ]
}
