{
  "name": "My TweetDeck",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://tweetdeck.twitter.com/*"
      ],
      "js": [
        "vendor/jquery.min.js",
        "config/config.js",
        "scripts/column.js",
        "scripts/modal.js",
        "scripts/notice.js",
        "scripts/item.js",
        "scripts/customize.js"
      ],
      "css": [
        "styles/style.css"
      ]
    }
  ]
}
